// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Job {
  id         String    @id @default(uuid())
  name       String
  queue      String    @default("default")
  data       Json
  created_at DateTime  @default(now())
  last_run   DateTime?
  next_run   DateTime?
}
